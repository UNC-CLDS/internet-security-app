<?xml version='1.0' encoding='utf-8'?>
<widget id="io.internetsecurityapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Sad Cyber Sam</name>
    <description>
        Main application for games
    </description>
    <author email="piccot@live.unc.edu" href="http://cordova.io">
       Daniel Piccot and Matt Hill
    </author>
    <content src="main_initial.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="DisallowOverscroll" value="true" />
        <preference name="KeepRunning" value="false" />
        <preference name="Fullscreen" value="true" />
        <preference name="Orientation" value="portrait" />
    </platform>
    <platform name="ios">
        <preference name="DisallowOverscroll" value="true" />
        <preference name="Orientation" value="portrait" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="Fullscreen" value="true" />
    </platform>
    <plugin name="cordova-plugin-console" />
    <plugin name="cordova-plugin-file" />
    <plugin name="cordova-plugin-file-transfer" />
    <plugin name="cordova-plugin-media" />
    <plugin name="cordova-plugin-nativeaudio" />
    <plugin name="cordova-plugin-screen-orientation" />
    <plugin name="cordova-plugin-whitelist" />
    <plugin name="cordova-plugin-wkwebview-engine" />
    <engine name="ios" spec="~4.5.3" />
</widget>
